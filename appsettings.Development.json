{
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AppConfig": {
    "StorageConnectionString": "UseDevelopmentStorage=true",
    "AdUserInfoPath": ".auth/me",
    "PollIntervalSec": "5",
    "PresenceExpirationMultiplier": "4"
  }
}
